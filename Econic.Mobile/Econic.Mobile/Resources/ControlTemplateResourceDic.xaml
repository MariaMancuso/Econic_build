<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary  xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:combobox="clr-namespace:Syncfusion.XForms.ComboBox;assembly=Syncfusion.SfComboBox.XForms">

    <Style TargetType="Button" x:Key="SecondaryBtn">
        <Setter Property="BackgroundColor" Value="#DFECF2"/>
        <Setter Property="TextColor" Value="#517486"/>
        <Setter Property="CornerRadius" Value="20"/>
        <Setter Property="HorizontalOptions" Value="Start"/>
        <Setter Property="HeightRequest" Value="30"/>
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="FontFamily" Value="QuicksandBold"/>
        <Setter Property="Padding" Value="15, 0, 15, 0"/>
    </Style>


</ResourceDictionary>