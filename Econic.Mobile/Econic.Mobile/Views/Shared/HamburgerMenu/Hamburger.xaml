<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Econic.Mobile.Views.Shared.HamburgerMenu.Hamburger"
             xmlns:pages="clr-namespace:Econic.Mobile.Views.Shared.HamburgerMenu">
  <MasterDetailPage.Master>
    <pages:HamburgerMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage BarBackgroundColor="WhiteSmoke">
      <x:Arguments>
        <pages:Dashboard />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>